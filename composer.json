{
  "name": "patchlevel/event-sourcing",
  "type": "library",
  "license": "MIT",
  "description": "todo",
  "keywords": [
    "event-sourcing"
  ],
  "homepage": "https://github.com/patchlevel/event-sourcing",
  "authors": [
    {
      "name": "Daniel Badura",
      "email": "d.m.badura@gmail.com"
    },
    {
      "name": "David Badura",
      "email": "d.a.badura@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "^7.4",
    "doctrine/dbal": "^3.0.0"
  },
  "require-dev": {
    "ext-pdo_sqlite": "^7.4",
    "patchlevel/coding-standard": "^1.0.0",
    "phpspec/prophecy-phpunit": "^2.0.1",
    "phpstan/phpstan": "^0.12.65",
    "phpunit/phpunit": "^9.5.0",
    "symfony/messenger": "^5.2.1",
    "symfony/var-dumper": "^5.2.1",
    "vimeo/psalm": "^4.3.2"
  },
  "suggest": {
    "symfony/messenger": "for an advanced message bus"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Patchlevel\\EventSourcing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Patchlevel\\EventSourcing\\Tests\\": "tests/"
    }
  }
}
